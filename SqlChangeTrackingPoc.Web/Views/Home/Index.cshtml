@using SqlChangeTrackingPoc.Models.DevTest

@model IEnumerable<DevTestModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<div>
    <a href="~/Home/Create" class="btn btn-primary">Create</a>
</div>
<table class="table table-responsive table-striped table-hover">
    <tr>
        <th>ID</th>
        <th>Affiliate Name</th>
        <th>Campaign Name</th>
        <th>Clicks</th>
        <th>Conversions</th>
        <th>Date</th>
        <th>Impressions</th>
        <th>Actions</th>
    </tr>
    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.ID
                </td>
                <td>
                    @item.AffiliateName
                </td>
                <td>
                    @item.CampaignName
                </td>
                <td>
                    @item.Clicks
                </td>
                <td>
                    @item.Conversions
                </td>
                <td>
                    @item.Date
                </td>
                <td>
                    @item.Impressions
                </td>
                <td>
                    <a href="~/Home/Delete/@item.ID" class="btn btn-danger btn-sm">&times;</a>
                    <a href="~/Home/Edit/@item.ID" class="btn btn-warning btn-sm">
                        <span class="glyphicon glyphicon-edit"></span>
                    </a>
                </td>
            </tr>
        }
    }
</table>